<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://restimport.xml.turi.oikeatoliot.fi" targetNamespace="http://restimport.xml.turi.oikeatoliot.fi"
           elementFormDefault="unqualified" attributeFormDefault="unqualified">
    <xs:element name="poikkeama">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="lahde">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="lahdejarjestelma" minOccurs="0" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeamailmoituksen lähdejärjestelmä.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="300" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="lahdeid" minOccurs="0" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeamailmoituksen tunniste lähdejärjestelmässä.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="1" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="tapahtumantiedot">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="id" minOccurs="0" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeamailmoituksen yksilöivä tunnistenumero TURI-järjestelmässä.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="1" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="sampohankenimi" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeamailmoitusta koskevan hankkeen nimi Väylän Sampo-järjestelmässä.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="300" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="sampohankeid" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeamailmoitusta koskevan hankkeen tunniste Väylän Sampo-järjestelmässä.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="32" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>

                            <xs:element name="tilaajanvastuuhenkilokayttajatunnus" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Hankeen tilaajan yhteyshenkilö (käyttäjätunnus).
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="256" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tilaajanvastuuhenkiloetunimi" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Hankeen tilaajan yhteyshenkilön etunimi..
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tilaajanvastuuhenkilosukunimi" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Hankeen tilaajan yhteyshenkilön sukunimi.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tilaajanvastuuhenkilosposti" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Hankeen tilaajan yhteyshenkilön sähköpostiosoite.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="254" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="sampourakkanimi" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeamailmoitusta koskevan urakan nimi Väylän Sampo-järjestelmässä.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="300" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="sampourakkaid" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeamailmoitusta koskevan urakan tunniste Väylän Sampo-järjestelmässä.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="32" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="urakanpaattymispvm" type="xs:date" minOccurs="0" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Urakan päättymisajankohta.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="urakkavaylamuoto" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>
                                        Urakan väylämuoto: tie/rautatie/vesi.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="Tie" />
                                        <xs:enumeration value="Rautatie" />
                                        <xs:enumeration value="Vesiväylä" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="urakkatyyppi" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>
                                        Urakan tyyppi.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <!-- Tieurakat -->
                                        <xs:enumeration value="hoito" />
                                        <xs:enumeration value="paallystys" />
                                        <xs:enumeration value="paikkaus" />
                                        <xs:enumeration value="tiemerkinta" />
                                        <xs:enumeration value="valaistus" />
                                        <xs:enumeration value="siltakorjaus" />
                                        <xs:enumeration value="tekniset-laitteet" />
                                        <!-- Vesiurakat -->
                                        <xs:enumeration value="vesivayla-hoito" />
                                        <xs:enumeration value="vesivayla-ruoppaus" />
                                        <xs:enumeration value="vesivayla-turvalaitteiden-korjaus" />
                                        <xs:enumeration value="vesivayla-kanavien-hoito" />
                                        <xs:enumeration value="vesivayla-kanavien-korjaus" />
                                        <!-- Rataurakat -->
                                        <xs:enumeration value="kunnossapito-paallysrakenne" />
                                        <xs:enumeration value="kunnossapito-sahkorata" />
                                        <xs:enumeration value="kunnossapito-turvalaite" />
                                        <xs:enumeration value="yllapito" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="elyalue" minOccurs="0" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        ELY-alue.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="UUD ELY" />
                                        <xs:enumeration value="VAR ELY" />
                                        <xs:enumeration value="PIR ELY" />
                                        <xs:enumeration value="KAS ELY" />
                                        <xs:enumeration value="KES ELY" />
                                        <xs:enumeration value="POS ELY" />
                                        <xs:enumeration value="EPO ELY" />
                                        <xs:enumeration value="POP ELY" />
                                        <xs:enumeration value="LAP ELY" />
                                        <xs:enumeration value="KAN ELY" />
                                        <xs:enumeration value="SV ELY" />
                                        <xs:enumeration value="MV ELY" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="alueurakkanro" minOccurs="0" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeamailmoitusta koskevan urakan alueurakkanumero.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="16" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>

                            <xs:element name="tyyppi" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>
                                        Poikkeaman tyyppi
                                        Syötä ilmoitusta koskevan poikkeaman tyyppi (voit valita useita vaihtoehtoja).

                                        8 = Työtapaturma
                                        16 = Muuu turvallisuuspoikkeama
                                        32 =
                                        Vaaratilanne
                                        64 = Turvallisuushavainto
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:long">
                                        <xs:enumeration value="8" />
                                        <xs:enumeration value="16" />
                                        <xs:enumeration value="32" />
                                        <xs:enumeration value="64" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tapahtumapvm" type="xs:date">
                                <xs:annotation>
                                    <xs:documentation>
                                        Tapahtuman päivä. Milloin tapaturma tai vaaratilanne tapahtui.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="tapahtumaaika" type="xs:time" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Tapahtuman kellonaika.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="poikkeamalaji" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        1 = Matkustajajunaliikenne
                                        2 = Tavarajunaliikenne
                                        4 = Muu junaliikenne
                                        8 = Ratatyö
                                        16 = Vaihtotyö
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                        <xs:enumeration value="4" />
                                        <xs:enumeration value="8" />
                                        <xs:enumeration value="16" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="junanumero" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Junanumero

                                        Tämä kenttä on käytössä, jos poikkeamalaji-kentällä on jokin seuraavista arvoista: 1,2,4
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="0" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="vaihtotyonvastuuyksikko" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Vaihtotyön vastuuyksikkö

                                        1 = Rautatielogistiikka
                                        2 = Matkustajaliikenne
                                        4 = Junaliikennöinti
                                        8 = Kunnossapito
                                        16 = Infrarakentaminen
                                        32 = Ulkopuolinen
                                        vaihtotyö

                                        Tämä kenttä on käytössä, jos poikkeamalaji-kentällä on jokin seuraavista arvoista: 16
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                        <xs:enumeration value="4" />
                                        <xs:enumeration value="8" />
                                        <xs:enumeration value="16" />
                                        <xs:enumeration value="32" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="syy" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Tapahtuman välitön syy

                                        1 = Tekninen vika
                                        2 = Virheellinen toiminta
                                        4 = Ulkopuolinen syy
                                        8 = Muu syy
                                        16 = Ei tiedossa
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                        <xs:enumeration value="4" />
                                        <xs:enumeration value="8" />
                                        <xs:enumeration value="16" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="teknisenviantyyppi" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Teknisen vian tyyppi

                                        1 = Vika liikkuvassa kalustossa
                                        2 = Vika radassa
                                        4 = Vika turvalaitteissa
                                        8 = Muu tekninen vika

                                        Tämä kenttä on käytössä, jos syy-kentän
                                        arvo on 1
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                        <xs:enumeration value="4" />
                                        <xs:enumeration value="8" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="virheentyyppi" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Virheellisen toiminnan tyyppi

                                        1 = Virhe yksikön kuljettamisessa
                                        2 = Virhe kulkutien turvaamisessa
                                        4 = Virhe ratatyön suojaamisessa
                                        8 = Muu virhetoiminto

                                        Tämä
                                        kenttä on käytössä, jos syy-kentän arvo on 2
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                        <xs:enumeration value="4" />
                                        <xs:enumeration value="8" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="ulkopuolisensyyntyyppi" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Ulkopuolisen syyn tyyppi

                                        1 = Tieliikenne
                                        2 = Este radalla
                                        4 = Rikollinen teko
                                        8 = Muu ulkopuolinen syy

                                        Tämä kenttä on käytössä, jos syy-kentän arvo on 4
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                        <xs:enumeration value="4" />
                                        <xs:enumeration value="8" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="muunsyynkuvaus" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Muun syyn kuvaus

                                        Tämä kenttä on käytössä, jos syy-kentän arvo on 8
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="kuvaus">
                                <xs:annotation>
                                    <xs:documentation>
                                        Vapaamuotoinen kuvaus

                                        Tämä kenttä on käytössä, jos syy-kentän arvo on 8
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="2000" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="tapahtumapaikka">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="tapahtumapaikkaradalla" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Tapahtumapaikka radalla

                                        1 = Tapahtui liikennepaikalla
                                        2 = Tapahtui liikennepaikkojen välillä
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="ratapaikka" type="ratapaikkaType" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Rataosan, liikennepaikan, tai liikennepaikanosan koodi.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="paikantarkenne" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Tarkempi tapahtumapaikka

                                        1 = Ratapiha
                                        2 = Rata-alue
                                        4 = Lastausalue
                                        8 = Asemalaituri
                                        16 = Piha-alue
                                        32 = Tiealue
                                        64 = Junakalusto
                                        128 = Terminaali
                                        256 = Halli
                                        512
                                        = Asema
                                        1024 = Tuotantolaitos
                                        2048 = Työmaa
                                        4096 = Tieliikenneajoneuvo
                                        8192 = Asiakkaan tilat
                                        16384 = Ravintola/kioski
                                        32768 = Toimisto
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                        <xs:enumeration value="4" />
                                        <xs:enumeration value="8" />
                                        <xs:enumeration value="16" />
                                        <xs:enumeration value="32" />
                                        <xs:enumeration value="64" />
                                        <xs:enumeration value="128" />
                                        <xs:enumeration value="256" />
                                        <xs:enumeration value="512" />
                                        <xs:enumeration value="1024" />
                                        <xs:enumeration value="2048" />
                                        <xs:enumeration value="4096" />
                                        <xs:enumeration value="8192" />
                                        <xs:enumeration value="16384" />
                                        <xs:enumeration value="32768" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="kmsijainti" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Tarkka sijainti ratametreinä, jos tapahtuma tapahtui liikennepaikkojen välillä.

                                        Tämä kenttä on käytössä, jos tapahtumapaikkaradalla-kentän arvo on 2
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="0" />
                                        <xs:maxInclusive value="9999999" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="paikka" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Tarkempi kuvaus tapahtumapaikasta
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tapahtumaalue" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        Tapahtuma-alue

                                        1 = Etelä-Suomi
                                        2 = Länsi-Suomi
                                        4 = Itä-Suomi
                                        8 = Pohjois-Suomi
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:enumeration value="1" />
                                        <xs:enumeration value="2" />
                                        <xs:enumeration value="4" />
                                        <xs:enumeration value="8" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="eureffinn" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:double">
                                        <xs:minInclusive value="0" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="eureffine" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:double">
                                        <xs:minInclusive value="0" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tienimi" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tienumero" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="0" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tieaosa" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="0" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tielosa" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="0" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tieaet" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="0" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="tielet" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="0" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="syytjaseuraukset">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="arviosyista" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
