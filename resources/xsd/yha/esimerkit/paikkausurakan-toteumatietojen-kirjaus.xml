<yha:paikkausurakan-toteumatietojen-kirjaus xmlns:yha="http://www.liikennevirasto.fi/xsd/yha">
    <yha:urakat>

        <!-- Sanomassa voi lähettää usean urakan tiedot. Urakka voi sisältää useita paikkaustoteumia. -->
        <yha:urakka>

            <!-- Urakan tunnistetiedot eri järjestelmissä. Harjan ja YHA:n tunnisteet ovat pakollisia. -->
            <yha:yha-id>YHA001</yha:yha-id>
            <yha:harja-id>HARJA777</yha:harja-id>
            <yha:sampo-id>SAMPO008</yha:sampo-id>

            <!-- Urakoitisjatiedot. Y-tunnus on pakollinen. -->
            <yha:urakoitsija>
                <yha:y-tunnus>1234567-0</yha:y-tunnus>
                <yha:nimi>Paikkaaja Oy</yha:nimi>
            </yha:urakoitsija>

            <!-- Urakkatyyppi on pakollinen tieto -->
            <!-- 1 = Hoidon alueurakka 2 = Päällystyksen palvelusopimukset 3 = Päällystysurakat 4 = Paikkausurakat -->
            <yha:urakkatyyppi>3</yha:urakkatyyppi>

            <!-- K O K O E L M A   T O T E U M I A  -->
            <yha:toimenpidetoteumat>

                <!-- Toteumia voi olla yksi tai useampia.
                Työmenetelmä on pakollinen tieto, jonka sisällön on vastattava enumeraatiota.
                Paikkauskohdasta täytyy antaa jotakin informaatiota. (TODO: saako tän määriteltyä skeemassa)
                Paikkauskohdasta voi antaa niin paljon informaatiota kuin tarvitsee. (TODO: saako tän määriteltyä skeemassa).
                -->
                <yha:toteuma>
                    <yha:harja-id>20202022</yha:harja-id>
                    <yha:tyomenetelma>22</yha:tyomenetelma>

                    <!-- Aloitus- ja lopetuspäivämäärät ovat pakollisia tietoja -->
                    <yha:aloituspaivamaara>2018-01-26</yha:aloituspaivamaara>
                    <yha:lopetuspaivamaara>2018-02-01</yha:lopetuspaivamaara>

                    <!-- TR-osoite, kaista ja ajorata ovat pakollisia tietoja. Kaistasta ja ajoradasta on enumeraatio (yhteinen päällystystoteuman kanssa).  -->
                    <yha:sijainti>
                        <yha:karttapaivamaara>2018-11-01+02:00</yha:karttapaivamaara>
                        <yha:tienumero>3</yha:tienumero>
                        <yha:aosa>3</yha:aosa>
                        <yha:aet>3</yha:aet>
                        <yha:losa>3</yha:losa>
                        <yha:let>3</yha:let>
                        <yha:ajorata>1</yha:ajorata>
                        <yha:kaista>21</yha:kaista>
                    </yha:sijainti>

                    <yha:kohta>
                        <!--Zero or more repetitions:-->
                        <yha:reuna>2</yha:reuna>
                        <!--Zero or more repetitions:-->
                        <yha:ajoura>3</yha:ajoura>
                        <yha:ajoura>4</yha:ajoura>
                        <!--Zero or more repetitions:-->
                        <yha:ajouravali>2</yha:ajouravali>
                        <!--Zero or more repetitions:-->
                        <yha:keskisauma>1</yha:keskisauma>
                    </yha:kohta>

                    <!-- Massatyyppi, massamenekki, raekoko, kuulamyllyluokka ja leveys eivät ole pakollisia.
                    Kuulamyllyluokka on enum, jota käyttää myös päällystystoteuman määrttely.
                    TODO: joihinkin työmenetelmiin liittyy pakollisuuksia.
                    TODO: leveydelle integer/metri ei liene riittävä tarkkuus? Entä massamenekille?-->
                    <yha:massatyyppi>12</yha:massatyyppi>
                    <yha:massamenekki>3</yha:massamenekki>
                    <yha:raekoko>3</yha:raekoko>
                    <yha:kuulamyllyluokka>3</yha:kuulamyllyluokka>
                    <yha:leveys>3</yha:leveys>

                    <!-- Materiaali ei ole pakollinen tieto.
                    Toimenpidetoteuma voi sisältää useita materiaalielementtejä.
                    Materiaalista voi kertoa kiviaineksen tyypin, kuulamyllyarvon luokan, litteysluvun luokan, sideaineen, sideainepitoisuuden ja lisäaineet.
                    TODO: Tämä on päällystysurakalle määritelty tietotyypi. Sopiiko paikkauksiin (hox. esim. sideaineen enum)?
                    TODO: Kaikista materiaalitiedoista ei ole enumeraatioita. Otetaanko vastaan mitä vaan?
                    -->
                    <yha:materiaalit>
                        <yha:materiaali>
                            <yha:kiviainesesiintyman-nimi>Kuivattu murske</yha:kiviainesesiintyman-nimi>
                            <yha:kiviaineksen-km-arvo>AN7</yha:kiviaineksen-km-arvo>
                            <yha:kiviaineksen-muotoarvo>FI10</yha:kiviaineksen-muotoarvo>
                            <yha:sideainetyyppi>2</yha:sideainetyyppi>
                            <yha:sideainepitoisuus>10</yha:sideainepitoisuus>
                            <yha:lisa-aineet>E711 ja natriumglutamaatti</yha:lisa-aineet>
                        </yha:materiaali>
                        <yha:materiaali>
                            <yha:kiviainesesiintyman-nimi>Bituminoitu murske</yha:kiviainesesiintyman-nimi>
                            <yha:kiviaineksen-km-arvo>AN10</yha:kiviaineksen-km-arvo>
                            <yha:kiviaineksen-muotoarvo>N10</yha:kiviaineksen-muotoarvo>
                        </yha:materiaali>
                        <yha:materiaali>
                            <yha:sideainetyyppi>1</yha:sideainetyyppi>
                            <yha:sideainepitoisuus>2</yha:sideainepitoisuus>
                        </yha:materiaali>
                    </yha:materiaalit>
                </yha:toteuma>

                <!-- Toinen toteuma-->
                <yha:toteuma>
                    <yha:harja-id>20202022</yha:harja-id>
                    <yha:tyomenetelma>31</yha:tyomenetelma>
                    <yha:aloituspaivamaara>2018-01-26</yha:aloituspaivamaara>
                    <yha:lopetuspaivamaara>2018-02-01</yha:lopetuspaivamaara>
                    <yha:sijainti>
                        <yha:karttapaivamaara>2018-11-01+02:00</yha:karttapaivamaara>
                        <yha:tienumero>3</yha:tienumero>
                        <yha:aosa>3</yha:aosa>
                        <yha:aet>3</yha:aet>
                        <yha:losa>3</yha:losa>
                        <yha:let>3</yha:let>
                        <yha:ajorata>1</yha:ajorata>
                        <yha:kaista>21</yha:kaista>
                    </yha:sijainti>
                    <yha:kohta>
                        <yha:reuna>1</yha:reuna>
                        <yha:reuna>2</yha:reuna>
                    </yha:kohta>
                    <yha:massatyyppi>12</yha:massatyyppi>
                    <yha:massamenekki>1</yha:massamenekki>
                    <yha:raekoko>3</yha:raekoko>
                    <yha:leveys>3</yha:leveys>
                    <yha:materiaalit>
                        <yha:materiaali>
                            <yha:kiviainesesiintyman-nimi>Bituminoitu murske</yha:kiviainesesiintyman-nimi>
                            <yha:kiviaineksen-km-arvo>AN10</yha:kiviaineksen-km-arvo>
                            <yha:kiviaineksen-muotoarvo>N10</yha:kiviaineksen-muotoarvo>
                        </yha:materiaali>
                    </yha:materiaalit>
                </yha:toteuma>

                <!-- Kolmas toteuma -->
                <yha:toteuma>
                    <yha:harja-id>20202022</yha:harja-id>
                    <yha:tyomenetelma>21</yha:tyomenetelma>
                    <yha:aloituspaivamaara>2018-01-26</yha:aloituspaivamaara>
                    <yha:lopetuspaivamaara>2018-02-01</yha:lopetuspaivamaara>
                    <yha:sijainti>
                        <yha:karttapaivamaara>2018-11-01+02:00</yha:karttapaivamaara>
                        <yha:tienumero>3</yha:tienumero>
                        <yha:aosa>3</yha:aosa>
                        <yha:aet>3</yha:aet>
                        <yha:losa>3</yha:losa>
                        <yha:let>3</yha:let>
                        <yha:ajorata>1</yha:ajorata>
                        <yha:kaista>21</yha:kaista>
                    </yha:sijainti>
                    <yha:kohta>
                        <yha:reuna>2</yha:reuna>
                        <yha:ajoura>3</yha:ajoura>
                        <yha:ajouravali>3</yha:ajouravali>
                        <yha:keskisauma>3</yha:keskisauma>
                    </yha:kohta>
                    <yha:massatyyppi>12</yha:massatyyppi>
                    <yha:massamenekki>3</yha:massamenekki>
                    <yha:raekoko>1</yha:raekoko>
                    <yha:leveys>1</yha:leveys>
                    <yha:materiaalit>
                        <yha:materiaali>
                            <yha:kiviainesesiintyman-nimi>Bituminoimaton sirotekiviaines</yha:kiviainesesiintyman-nimi>
                            <yha:kiviaineksen-km-arvo>AN19</yha:kiviaineksen-km-arvo>
                            <yha:kiviaineksen-muotoarvo>F7</yha:kiviaineksen-muotoarvo>
                            <yha:sideainetyyppi>3</yha:sideainetyyppi>
                            <yha:sideainepitoisuus>1.2</yha:sideainepitoisuus>
                            <yha:lisa-aineet>Atsovärit</yha:lisa-aineet>
                        </yha:materiaali>
                    </yha:materiaalit>
                </yha:toteuma>
            </yha:toimenpidetoteumat>
        </yha:urakka>
    </yha:urakat>
</yha:paikkausurakan-toteumatietojen-kirjaus>