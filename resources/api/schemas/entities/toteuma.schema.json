{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "id": "urn:harja",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "tunniste": {
      "id": "urn:harja/toteumat/0/toteuma/tunniste",
      "type": "object",
      "additionalProperties": false,
      "$ref": "file:resources/api/schemas/entities/tunniste.schema.json",
      "required": true
    },
    "alueurakkanumero": {
      "id": "urn:harja/ilmoitukset/0/ilmoitus/alueurakkanumero",
      "type": "integer",
      "required": false
    },
    "sopimusId": {
      "id": "urn:harja/toteumat/0/toteuma/sopimusId",
      "type": "integer"
    },
    "alkanut": {
      "id": "urn:harja/toteumat/0/toteuma/alkanut",
      "type": "string",
      "format": "date-time",
      "required": true
    },
    "paattynyt": {
      "id": "urn:harja/toteumat/0/toteuma/paattynyt",
      "type": "string",
      "format": "date-time",
      "required": true
    },
    "suorittaja": {
      "id": "urn:harja/suorittaja",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "nimi": {
          "id": "urn:harja/suorittaja/nimi",
          "type": "string"
        },
        "ytunnus": {
          "id": "urn:harja/suorittaja/ytunnus",
          "type": "string"
        }
      }
    },
    "toteumatyyppi": {
      "id": "urn:harja/toteumat/0/toteuma/kokonaishintainenTyo",
      "required": true,
      "enum": [
        "yksikkohintainen",
        "kokonaishintainen",
        "akillinen-hoitotyo",
        "lisatyo",
        "muutostyo",
        "vahinkojen-korjaukset"
      ]
    },
    "lisatieto": {
      "id": "urn:harja/toteumat/0/toteuma/lisatieto",
      "type": "string",
      "required": false
    },
    "tehtavat": {
      "id": "urn:harja/toteumat/0/toteuma/tehtavat/",
      "type": "object",
      "additionalProperties": false,
      "$ref": "file:resources/api/schemas/entities/tehtavat.schema.json"
    },
    "materiaalit": {
      "id": "urn:harja/toteumat/0/toteuma/materiaalit/",
      "type": "object",
      "additionalProperties": false,
      "$ref": "file:resources/api/schemas/entities/materiaalit.schema.json"
    },
    "tiesijainti": {
      "id": "urn:harja/toteumat/0/toteuma/muutostiedot/",
      "type": "object",
      "additionalProperties": false,
      "$ref": "file:resources/api/schemas/entities/tiesijainti.schema.json"
    },
    "muutostiedot": {
      "id": "urn:harja/toteumat/0/toteuma/muutostiedot/",
      "type": "object",
      "additionalProperties": false,
      "$ref": "file:resources/api/schemas/entities/muutostiedot.schema.json"
    }
  }
}