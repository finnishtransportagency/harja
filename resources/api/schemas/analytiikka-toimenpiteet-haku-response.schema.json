{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "toimenpiteet": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "toimenpide": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "nimi": { "type": "string" },
              "koodi": { "type": "string" },
              "emo": { "type": "integer" },
              "taso": { "type": "integer" },
              "luotu": { "type": "string", "format": "date-time" },
              "muokattu": { "type": "string", "format": "date-time" },
              "luoja": { "type": "integer" },
              "muokkaaja": { "type": "integer" },
              "poistettu": { "type": "boolean" },
              "tuotenumero": { "type": "integer" },
              "piilota": { "type": "boolean" }
            },
            "required": ["id", "nimi", "koodi", "emo", "taso", "luotu", "muokattu", "luoja", "muokkaaja", "poistettu", "tuotenumero", "piilota"]
          }
        },
        "required": ["toimenpide"]
      }
    }
  },
  "required": ["toimenpiteet"]
}
