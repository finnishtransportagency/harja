{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "additionalProperties": false,
  "description": "Poistaa talvihoitoreitit ulkoisen-id:n perusteella. Listaa kaikki poistettavat talvihoitoreitit arrayna.",
  "properties": {
    "talvihoitoreittien-tunnisteet": {
      "type": "array",
      "minItems": 1,
      "maxItems": 1000,
      "required": true,
      "items": {
        "type": "string"
      }
    }
  }
}
